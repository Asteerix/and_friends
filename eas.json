{
  "cli": {
    "version": ">= 3.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium",
        "simulator": false
      }
    },
    "production": {
      "ios": {
        "resourceClass": "m-medium",
        "buildConfiguration": "Release",
        "distribution": "store",
        "cache": {
          "disabled": false,
          "key": "ios-production-v3"
        }
      },
      "android": {
        "buildType": "app-bundle"
      },
      "env": {
        "EXPO_NO_DOTENV": "1"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "apoltavtseef@gmail.com",
        "ascAppId": "6749200050",
        "appleTeamId": "2798WUY9MP"
      },
      "android": {
        "serviceAccountKeyPath": "./path-to-google-service-account-key.json",
        "track": "production"
      }
    }
  }
}